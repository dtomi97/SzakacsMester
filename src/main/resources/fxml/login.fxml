<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="760.0" prefWidth="1070.0" style="&#10;"
            xmlns="http://javafx.com/javafx/2.2" fx:controller="control.RegAndLogController">
    <ImageView fitHeight="765.0" fitWidth="1075.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../images/epic.jpg"/>
    </ImageView>
    <AnchorPane id="" fx:id="loginImage" layoutX="207.0" layoutY="-15.0" prefHeight="557.0" prefWidth="678.0">
        <TextField fx:id="userName" layoutX="136.0" layoutY="133.0" prefHeight="27.0" prefWidth="354.0"
                   promptText="Felhasználónév"/>
        <PasswordField fx:id="pwd" layoutX="133.0" layoutY="187.0" prefHeight="27.0" prefWidth="354.0"
                       promptText="Jelszó"/>
        <Button id="Login" fx:id="loginButton" blendMode="SRC_OVER" layoutX="136.0" layoutY="299.0"
                mnemonicParsing="false" onAction="#handleLoginBtn" prefWidth="146.0" styleClass="login"
                text="Bejelentkezés"/>
        <Button id="Register" fx:id="registrationButton" blendMode="SRC_OVER" layoutX="331.0" layoutY="299.0"
                mnemonicParsing="false" onAction="#handleRegistration" prefWidth="156.0" text="Regisztráció" styleClass="login" />
        <Label fx:id="feedbackLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="300.0" layoutY="250.0" minWidth="200.0" prefHeight="17.0" prefWidth="400.0" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
    </AnchorPane>
    <stylesheets>
        <URL value="@../StyleSheet.css"/>
    </stylesheets>
</AnchorPane>
